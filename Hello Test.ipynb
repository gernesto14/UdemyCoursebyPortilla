    "def myfunc(s='Anthropomorphism'):\n",
    "    mylist = []\n",
    "    i = 0\n",
    "    for x in s:\n",
    "        if i%2 != 0:\n",
    "            mylist.append(x.upper())\n",
    "        else:\n",
    "            mylist.append(x.lower())\n",
    "        i += 1\n",
    "    return print(''.join(mylist))\n"
